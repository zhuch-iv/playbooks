version: "3.9"
services:
  jupyter:
    image: jupyter/datascience-notebook:latest
    container_name: jupyter
    ports:
      - 8888:8888
    environment:
      JUPYTER_ENABLE_LAB: "yes"
      JUPYTER_TOKEN: "{{ root_pass }}"
    volumes:
        - jupyter:/home/jovyan/work
        
volumes:
  jupyter:
